# To sort on expanded paths, first separate the expanded paths into their own
# column (as the 4th whitespace-delimited column of text), then sort with `C-u 4
# M-x sort-fields'.

hash -d  l=${HOME}/lo
hash -d  s=${HOME}/syscfg
hash -d  g=${HOME}/prog/foreign/git
hash -d  m=${HOME}/prog/melby

hash -d nixp=${HOME}/.nix-profile
hash -d nix=/nix/var/nix
